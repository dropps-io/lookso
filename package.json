    "@mui/base": "^5.0.0-alpha.93",
    "@mui/material": "^5.10.2",
    "@mui/system": "^5.10.1",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/react": "^13.3.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "emoji-picker-react": "^3.6.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
